
<% include ../layout %>

<body>
    <!-- Sidenav -->
    <% include ../partials/sidebar %>
    <!-- Main content -->
    <div class="main-content">
        <!-- Top navbar -->
        <% include ../partials/navbar %>
        <!-- Header -->
        <% include ../partials/header %>
        <!-- Page content -->
        <div style="justify-content: center !important" class="row mt-4">
            <div class="col-xl-11 mb-5 mb-xl-0">
                <div class="card shadow">
                    <% include ../partials/message %>
                    <div class="card-header border-0">
                        <div class="row align-items-center">
                            <div class="col">
                                <h3 class="mb-0">Consultas Cancelados do Paciente</h3>
                            </div>
                            <a  id="btn-search" class="btn btn-primary"><img src="/img/icons/common/filter.svg">Filtrar</a>
                        </div>
                    </div>
                    <div id="campo-buscar">
                            <div class="col-lg-10" >
                                    <label class="form-control-label" for="input-email">Data da Consulta</label>

                            <form class="form-search-name">
                                <input type="text" name="nameMaster" class="form-control form-control-alternative"
                                    placeholder="Digite o nome">
                                <a class="btn btn-primary" id="search-name-master"><img src="/img/icons/common/search.svg">Buscar</a>
                            </form>
                            </div>
                        </div>
                    <div class="table-responsive">
                        <table class="table table-hover align-items-center table-md">
                            <thead class="thead-light">
                                <tr>
                                    <th scope="col">Nome Paciente</th>
                                    <th scope="col">Data da consulta</th>
                                    <th scope="col-5">Data do Cancelamento</th>
                                    <th scope="col">Nome Atendente</th>

                                    <th ></th>
                                    <th ></th>

                                    <!-- <th scope="col">Bounce rate</th> -->
                                </tr>
                            </thead>
                            <tbody id="table-master">
                                <% if(result.length > 0 ) {%>
                                <% for (var i = 0; i < result.length; i++) { %>
                                <tr>
                                    <th scope="row">
                                        <%= result[i].consultPatient.name %>
                                    </th>
                                    <td>
                                        <%= moment(result[i].dateStart).format('DD/MM/YYYY HH:mm') %>
                                    </td>
                                    <td>
                                        <%= moment(result[i].updateAt).format('DD/MM/YYYY HH:mm') %>
                                    </td>
                                    <td>
                                            <%= result[i].consultTrainee.name %>

                                    </td>
                               


                                    <% } %>
                                    <% } %>
                                    <!-- <td>
                              <i class="fas fa-arrow-up text-success mr-3"></i> 46,53%
                            </td> -->
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer -->
        <% include ../partials/footer %>

    </div>
    <!-- Argon Scripts -->
    <!-- Core -->
    <% include ../partials/scripts %>

</body>

</html>